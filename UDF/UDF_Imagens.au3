	$Extract_Sombra_hWnd_Main &= ''
	$Extract_Sombra_hWnd_Main = _WinAPI_Base64Decode($Extract_Sombra_hWnd_Main)
	Local $tSource = DllStructCreate('byte[' & BinaryLen($Extract_Sombra_hWnd_Main) & ']')
	DllStructSetData($tSource, 1, $Extract_Sombra_hWnd_Main)
