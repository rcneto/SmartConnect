		Local Const $hFile = FileOpen($sSavePath & "\Sombra_hWnd_Main.png", 18)
		If @error Then Return SetError(1, 0, 0)
		FileWrite($hFile, $bString)
		FileClose($hFile)
	EndIf
	Return $bString
EndFunc   ;==>_Extract_Sombra_hWnd_Main

