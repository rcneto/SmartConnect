	Local Const $bString = Binary(_WinAPI_Base64Decode($Extract_Logo_SmartConnect))
	If $bSaveBinary Then
		Local Const $hFile = FileOpen($sSavePath & "\Logo_SmartConnect.png", 18)
		If @error Then Return SetError(1, 0, 0)
		FileWrite($hFile, $bString)
		FileClose($hFile)
	EndIf
	Return $bString
EndFunc   ;==>_Extract_Logo_SmartConnect

